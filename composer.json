{
    "name": "gai871013/LaravelManage",
    "description": "基于Laravel的管理系统,致力于做一个五站合一(电脑站 + 手机站 + 微信公众平台 + APP + 小程序 五合一)的系统",
    "keywords": ["LaravelManage"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.0",
        "barryvdh/laravel-ide-helper": "^2.4",
        "fideloper/proxy":"~3.3",
        "guzzlehttp/guzzle": "^6.2",
        "intervention/image": "^2.3",
        "laracasts/flash": "^3.0",
        "laravel/framework": "5.5.*",
        "laravel/tinker": "~1.0",
        "overtrue/laravel-lang": "~3.0",
        "overtrue/laravel-ueditor": "^1.2",
        "overtrue/laravel-wechat": "^4.0",
        "predis/predis": "^1.1",
        "prettus/l5-repository": "^2.6",
        "prettus/laravel-validation": "^1.1",
        "rap2hpoutre/laravel-log-viewer": "^0.10.0",
        "spatie/laravel-fractal": "^5.0",
        "spatie/laravel-pjax": "^1.3"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "@php artisan package:discover",
            "php artisan optimize",
            "php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan package:discover",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
