/**index.wxss**/

page {
  background-image: url(data:image/png;base64,);
  background-size: 100% auto;
  background-position: 0 0;
  background-repeat: no-repeat;
}

.userinfo-avatar {
  width: 144rpx;
  height: 144rpx;
  border: 6rpx solid #f2f2f2;
  margin: 124rpx auto 66rpx;
  display: block;
  border-radius: 10rem;
  font-size: 28rpx;
  line-height: 1.2;
}

.userinfo-nickname {
  text-align: center;
  width: 100%;
  display: block;
}

.title, .body {
  width: 100%;
  display: block;
  text-align: center;
  color: #a1a1a2;
}

.title {
  font-size: 37rpx;
  margin: 72rpx auto 17rpx;
}

.body {
  font-size: 26rpx;
  margin-bottom: 73rpx;
}

.create {
  background: #fea000;
  width: 158rpx;
  height: 158rpx;
  border-radius: 10rem;
  line-height: 130rpx;
  font-size: 140rpx;
  color: #fff;
  box-shadow: 5rpx 5rpx 40rpx rgba(0, 0, 0, 0.2);
}

/* 已填写信息 */

.box .avatar {
  width: 120rpx;
  height: 120rpx;
  border-radius: 5rem;
  float: right;
  margin-top: 40rpx;
  margin-right: 60rpx;
}

.box .nickname {
  float: left;
  margin-top: 70rpx;
  height: 50rpx;
  border-left: 6rpx solid #37cf59;
  text-indent: 26rpx;
  font-size: 30rpx;
  color: #060606;
  line-height: 50rpx;
}

.box .form-group {
  height: 75rpx;
  line-break: 75rpx;
}

.box .form-group image {
  height: 40rpx;
  max-width: 100%;
  margin-top: 5rpx;
}

.box .form-group .col-md-1 {
  float: left;
}

.box .form-group .col-md-11 text {
  font-size: 30rpx;
  line-height: 1.5;
}

.color-1 {
  color: #0ca3ff;
}

.color-2 {
  color: #020202;
}

.color-3 {
  color: #8f8f8f;
}

.view-box {
  padding: 35rpx;
  border: 1rpx solid #dfdfde;
  border-left: 0;
  border-right: 0;
  background: #fff;
  margin-top: 30rpx;
  color: #878787;
  font-size: 28rpx;
}

.view-box .col-md-6 {
  text-align: center;
}

.view-box image {
  max-height: 40rpx;
  margin: 0 auto;
  max-width: 100%;
}
